{
  "name": "Object Equality",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["debugger"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["strictEquality.js"]
    }
  ]
}
